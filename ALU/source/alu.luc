module alu (
    input a[16],
    input b[16],
    input alufn[6],
    
    output out[16]
  ) {
  
  // Functions
  
    
  always {   
    case (alufn[5:4]) {
      2b00:
        out = math_unit.out;
        
      2b01:
        out = boolean_unit.out;
  
      2b10:
        out = shift_unit.out;
      
      2b11:
        out = compare_unit.out;
        
      default:
        out = 16h0000;
    }
  }
}
